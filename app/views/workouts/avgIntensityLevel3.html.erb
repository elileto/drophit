

  <h1> Workouts with Level 3 Intensity </h1>
  <br>

<div class="row">
    <% @intensity.each do |p| %>
      <div class="col-lg-4 col-md-6">
      <div class="card">
      <%= image_tag(p.featureImage, :class => "card-img-top") %>
      <div class="card-body">
        <h5 class="card-title"><%= p.name %></h5>
        <h2><span class="badge badge-primary badge-pill"> Intensity Level: <%= p.avgIntensity %></span> </h2>


      </div>
      </div>
      </div>
    <%end%>
</div>
